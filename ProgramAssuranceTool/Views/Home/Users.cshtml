@model ProgramAssuranceTool.ViewModels.Home.AboutViewModel

@{
    ViewBag.Title = "About";
}


<div class="page-header">  
    <h1>About</h1>
</div>

    <p>
        ver. @Model.Version - Release Date 29-JUN-2014 by the CPER Team.
    </p>
       
    <ul class="nav nav-tabs nav-justified">
        <li class="active" id="tabLink1"><a href="#tab1" data-toggle="tab">My Groups</a></li>    
        <li id="tabLink2"><a href="#tab2" data-toggle="tab">Users</a></li>
        <li id="tabLink3"><a href="#tab3" data-toggle="tab">System Info</a></li>
    </ul>

    <div class="tab-content" id="tabs">
 
        <div class="well tab-pane active" id="tab1">
            
            <div id="info">
                @Html.Raw( ViewData[ "groups" ] )
            </div>

        </div>

        <div class="well tab-pane" id="tab2">

            <div id="users">
                @Html.Raw( ViewData[ "admins" ] )
            </div>

        </div>

        <div class="well tab-pane" id="tab3">
            
            <div id="info">
                @Html.Raw( ViewData[ "about" ] )
            </div>

        </div>
    </div>


